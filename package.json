{
  "name": "educa-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --port=4201",
    "build": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "compodoc": "node -e \"fs.rmSync('documentacion', { recursive: true, force: true })\" && compodoc -p tsconfig.doc.json -s -w -o --port 8080",
    "start:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --configuration production --port=4202",
    "convert-images": "node scripts/convert-images.mjs",
    "serve:ssr:educa-angular": "node dist/educa-angular/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@angular/ssr": "^21.1.0",
    "@ng-web-apis/speech": "^5.0.3",
    "@ngrx/operators": "^21.0.1",
    "@ngrx/signals": "^21.0.1",
    "@primeng/themes": "^21.0.4",
    "bootstrap-icons": "^1.13.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.10",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.2",
    "@analogjs/vitest-angular": "^1.14.0",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@compodoc/compodoc": "^1.1.25",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^26.0.0",
    "prettier": "^3.8.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2",
    "vite": "^6.0.0",
    "vitest": "^4.0.0"
  }
}
